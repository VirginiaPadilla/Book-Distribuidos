@Article{Ratnasamy2001,
  author     = {Ratnasamy, Sylvia and Francis, Paul and Handley, Mark and Karp, Richard and Shenker, Scott},
  journal    = {SIGCOMM Comput. Commun. Rev.},
  title      = {A Scalable Content-Addressable Network},
  year       = {2001},
